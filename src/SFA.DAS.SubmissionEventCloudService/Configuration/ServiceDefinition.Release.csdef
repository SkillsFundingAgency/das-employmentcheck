<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="SFA.DAS.SubmissionEventCloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2015-04.2.6">
  <WorkerRole name="SubmissionEventWorkerRole" vmsize="Standard_D1_v2">
    <Runtime executionContext="elevated" />
    <Startup>
      <Task commandLine="StartUp\IIS.bat" executionContext="elevated" taskType="background"></Task>
    </Startup>
    <ConfigurationSettings>
      <Setting name="LoggingRedisConnectionString"/>
      <Setting name="LoggingRedisKey"/>
      <Setting name="InstrumentationKey" />
      <Setting name="EnvironmentName" />
      <Setting name="ConfigurationStorageConnectionString" />
      <Setting name="TokenServiceCertificateThumbprint" />
    </ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>
